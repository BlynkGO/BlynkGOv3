namespace AirConditioner {
  // คำสั่ง ปิด Air
  void OFF(){
    IrSender.sendRaw(airOFF, sizeof(airOFF) / sizeof(airOFF[0]) /*ความยาวของ Raw Data*/, 38 /*ความถี่ IR */ );
  }
  
  // สำหรับส่ง คำสั่ง จาก IR ไปยัง Air Conditioner เพื่อตั้งอุณหภูมิให้ Air Conditioner
  void level(uint8_t level){
    level = constrain(level,16,30);   // ให้ level ต่ำสุดที่ 16, สูงสุดที่ 30
    IrSender.sendRaw(airArray[level - 16], sizeof(airLevel16) / sizeof(airLevel16[0])  /*ความยาวของ Raw Data*/, 38 /*ความถี่ IR */ );
    
  }
}
